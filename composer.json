{
	"name": "codename/composer-requirejs-config",
	"description": "Generates a requireJS config js file from npm/bower assets",
  "version": "0.0.1",
  "type": "composer-plugin",
	"autoload": {
		"psr-4": {"codename\\ComposerRequirejsConfig\\": "src/"}
	},
	"extra": {
		"class": "codename\\ComposerRequirejsConfig\\Plugin"
	},
	"require": {
		"composer-plugin-api": "^1.0",
		"composer/installers": "^1.0"
	}
}